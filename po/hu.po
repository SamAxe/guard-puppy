#: protocoldb.cpp:1200
msgid ""
"XML Parse error:\n"
"%1"
msgstr ""
"XML feldolgozási hiba:\n"
"%1"

#: protocoldb.cpp:1202
msgid "'protocol' tag requires a 'name' attribute, but none was found."
msgstr "A 'protokoll' megadásához 'név' jellemzőre is szükség van, de ez nem található."

#: protocoldb.cpp:1204
msgid "'threat' attribute has an unrecognised value."
msgstr "A 'fenyegetés' jellemző érvénytelen értékkel rendelkezik."

#: protocoldb.cpp:1206
msgid "'falsepos' attribute has an unrecognised value."
msgstr "A 'falsepos' jellemző érvénytelen értékkel rendelkezik."

#: protocoldb.cpp:1208
msgid "'port' element requires a 'portnum' attribute, but none was found."
msgstr "A 'port' megadásához a 'portszám' értékre is szükség van, de ez nem található."

#: protocoldb.cpp:1210
msgid "'portnum' attribute is not a valid unsigned integer."
msgstr "A 'portszám' érték nem egész szám."

#: protocoldb.cpp:1212
msgid "'portrange' element requires a 'start' attribute, but none was found."
msgstr "A 'port tartomány' megadásához a 'kezdő' értékre is szükség van, de ez nem található."

#: protocoldb.cpp:1214
msgid "'start' attribute is not a valid unsigned integer."
msgstr "A 'kezdő' érték nem egész szám."

#: protocoldb.cpp:1216
msgid "'portrange' element requires a 'end' attribute, but none was found."
msgstr "A 'port tartomány' megadásához a 'befejező' értékre is szükség van, de ez nem található."

#: protocoldb.cpp:1218
msgid "'end' attribute is not a valid unsigned integer."
msgstr "A befejező' érték nem egész szám."

#: protocoldb.cpp:1220
msgid "'start' attribute must be greater than 'end' attribute."
msgstr "A 'kezdő' értéknek kisebbnek kell lennie a 'bejefező' értéknél."

#: protocoldb.cpp:1225
msgid "'source' attribute must be one of 'client', 'server' or 'host'."
msgstr "A 'forrás' jellemző 'kliens', 'szerver', vagy 'hoszt' lehet."

#: protocoldb.cpp:1230
msgid "'dest' attribute must be one of 'client', 'server' or 'host'."
msgstr "A 'cél' jellemző 'kliens', 'szerver', vagy 'hoszt' lehet."

#: protocoldb.cpp:1232
msgid "'type' element requires a 'value' attribute, but none was found."
msgstr "A 'típus' elemhez 'érték' jellemzőre van szükség, de ez nem található."

#: protocoldb.cpp:1234
msgid "'value' attribute is not a valid unsigned integer."
msgstr "Az 'érték' jellemző nem egész szám."

#: protocoldb.cpp:1236
msgid "'code' attribute is not a valid unsigned integer."
msgstr "A 'kód' jellemző nem egész szám."

#: protocoldb.cpp:1238
msgid "'class' attribute has an unrecognised value."
msgstr "Az 'osztály' jellemző nem érvényes értékkel rendelkezik."

#: protocoldb.cpp:1240
msgid "'ip' element requires a 'protocol' attribute, but none was found."
msgstr "Az 'ip' értékhez 'protokoll' jellemzőre van szükség, de ez nem található."

#: protocoldb.cpp:1242
msgid "'protocol' attribute is not a valid unsigned integer."
msgstr "A 'protokoll' érték nem egész szám."

#: protocoldb.cpp:1244
msgid "'protocol' attribute is out of range. (Must be 8 bit)."
msgstr "A 'protokoll' jellemző nem a tartományban van. (8 bitnek kell lennie)"

#: protocoldb.cpp:1247
msgid "Unknown error. (You should never see this)."
msgstr "Ismeretlen hiba. (Ilyet még nem láttál)"

